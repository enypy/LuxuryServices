{% extends "@EasyAdmin/page/content.html.twig" %}
{% block page_title %}Luxury Services | Dashboard
{% endblock %}
{% block content %}
	{{ ux_controller_link_tags() }}

	<div>
		<div class="d-flex justify-content-center">
			<h1>Stats</h1>
		</div>

		<div class="d-flex">
			<div class="container">
				<div class="row">
					<div class="col-4">
						{{ render_chart(messagesChart) }}
					</div>
					<div class="col-4">
						{{ render_chart(mixedChart) }}
					</div>
					<div class="col-4">
						{{ render_chart(applicationsChart) }}
					</div>
				</div>
			</div>
		</div>
	</div>
	{{ importmap() }}
{% endblock %}
