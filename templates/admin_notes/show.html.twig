{% extends 'base.html.twig' %}

{% block title %}AdminNotes{% endblock %}

{% block body %}
    <h1>AdminNotes</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ admin_note.id }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ admin_note.content }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ admin_note.updatedAt ? admin_note.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ admin_note.createdAt ? admin_note.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_notes_index') }}">back to list</a>

    <a href="{{ path('app_admin_notes_edit', {'id': admin_note.id}) }}">edit</a>

    {{ include('admin_notes/_delete_form.html.twig') }}
{% endblock %}
