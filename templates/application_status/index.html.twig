{% extends 'base.html.twig' %}

{% block title %}ApplicationStatus index{% endblock %}

{% block body %}
    <h1>ApplicationStatus index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>StatusValue</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for application_status in application_statuses %}
            <tr>
                <td>{{ application_status.id }}</td>
                <td>{{ application_status.statusValue }}</td>
                <td>
                    <a href="{{ path('app_application_status_show', {'id': application_status.id}) }}">show</a>
                    <a href="{{ path('app_application_status_edit', {'id': application_status.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_application_status_new') }}">Create new</a>
{% endblock %}
